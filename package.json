{
  "name": "sip-caller",
  "version": "2.0.0",
  "main": "index.js",
  "license": "MIT",
  "repository": "https://github.com/gazzati/sip-caller.git",
  "author": "gazzati <gazzaevtimur@gmail.com>",
  "scripts": {
    "build": "gulp build",
    "dev": "cross-env nodemon",
    "dev:consumer": "ts-node-dev --respawn --transpile-only --quiet src/processes/consumer.ts",
    "dev:processor": "ts-node-dev --respawn --transpile-only --quiet src/processes/processor.ts",
    "dev:api": "ts-node-dev --respawn --transpile-only --quiet src/processes/api.ts",
    "format": "prettier --write \"src/\"",
    "lint": "eslint src/**/*.ts --fix",
    "type": "tsc --project tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@gazzati/eslint-config-node": "^1.0.8",
    "@sentry/node": "^7.80.1",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.96",
    "@types/merge-deep": "^3.0.3",
    "@types/module-alias": "^2.0.3",
    "@types/node": "^20.8.10",
    "@types/uuid": "^9.0.7",
    "@types/websocket": "^1.0.8",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.53.0",
    "gulp": "^4.0.2",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-swc": "^1.3.2",
    "lint-staged": "^15.0.2",
    "module-alias": "^2.2.3",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5",
    "simple-pre-commit": "^1.2.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@fastify/sensible": "^5.5.0",
    "axios": "^1.6.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "merge-deep": "^3.0.3",
    "uuid": "^9.0.1",
    "websocket": "^1.0.34"
  },
  "simple-pre-commit": "npx lint-staged",
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  }
}
